    "prepublish": "node ../common/copy-readme.js && npm run rebuild",
    "start": "node dist/index.js",
    "bns": "npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfe84/rlay.git"
  },
  "keywords": [
