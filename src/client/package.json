{
  "name": "rlay",
  "version": "1.4.3",
  "description": "Relay HTTP requests through a server to your dev machine. A self hosted alternative to ngrok",
  "main": "dist/index.js",
  "dependencies": {
    "command-line-usage": "^6.1.1",
    "dotenv": "^10.0.0",
    "socket.io-client": "^4.1.1",
    "uuid": "^8.3.2",
    "yaclip": "^1.2.1",
    "rlay-common": "file:../common"
  },
  "devDependencies": {
    "@types/command-line-usage": "^5.0.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/should": "^13.0.0",
    "@types/uuid": "^8.3.1",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "testdouble": "^3.16.1",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "test": "mocha --require ts-node/register ./test/**/*.ts",
    "build": "tsc",
    "clean": "rimraf dist/",
